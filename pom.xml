<project xmlns="https://maven.apache.org/POM/4.0.0" xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>adservi.system</groupId>
  <artifactId>adservi.system</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>ADSERVI Pendencias</name>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <resources>
      <resource>
        <directory>src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.13.0</version>
        <configuration>
          <release>24</release>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.sun.xml.ws</groupId>
        <artifactId>jaxws-maven-plugin</artifactId>
        <version>4.0.3</version>
        <executions>

          <!-- WSDL 1 -->
          <execution>
            <id>generate-ws-client-1</id>
            <goals>
              <goal>wsimport</goal>
            </goals>
            <configuration>
              <wsdlUrls>
                <wsdlUrl></wsdlUrl>
              </wsdlUrls>
              <keep>true</keep>
              <packageName>util.webservices.fechamentos</packageName>
              <sourceDestDir>src</sourceDestDir>
            </configuration>
          </execution>

          <!-- WSDL 2 -->
          <execution>
            <id>generate-ws-client-2</id>
            <goals>
              <goal>wsimport</goal>
            </goals>
            <configuration>
              <wsdlUrls>
                <wsdlUrl></wsdlUrl>
              </wsdlUrls>
              <keep>true</keep>
              <packageName></packageName>
              <sourceDestDir>src/main/java</sourceDestDir>
            </configuration>
          </execution>

          <!-- WSDL 3 -->
          <execution>
            <id>generate-ws-client-3</id>
            <goals>
              <goal>wsimport</goal>
            </goals>
            <configuration>
              <wsdlUrls>
                <wsdlUrl></wsdlUrl>
              </wsdlUrls>
              <keep>true</keep>
              <packageName></packageName>
              <sourceDestDir>src/main/java</sourceDestDir>
            </configuration>
          </execution>

        </executions>
      </plugin>
      
      
      
      
      
    </plugins>
  </build>
